{
    "name": "gatsby-remark-makecode",
    "version": "1.0.7",
    "peerDependencies": {
        "sharp": "^0.27.2"
    },
    "dependencies": {
        "@types/node": "^14.14.32",
        "hast-util-to-html": "^7.1.2",
        "mdast-util-to-hast": "^10.2.0",
        "puppeteer": "^8.0.0",
        "remark": "^13.0.0",
        "typescript": "^4.2.3",
        "unist-util-find": "^1.0.2",
        "unist-util-visit": "^2.0.3"
    },
    "devDependencies": {
        "@semantic-release/git": "^9.0.0",
        "sharp": "^0.27.2"
    },
    "main": "dist/index.js",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch"
    },
    "engines": {
        "node": ">=12.13.0"
    },
    "release": {
        "branch": "main",
        "branches": [
            "main"
        ],
        "plugins": [
            [
                "@semantic-release/commit-analyzer",
                {
                    "preset": "angular",
                    "releaseRules": [
                        {
                            "type": "doc",
                            "release": "patch"
                        },
                        {
                            "type": "fix",
                            "release": "patch"
                        },
                        {
                            "type": "patch",
                            "release": "patch"
                        },
                        {
                            "type": "minor",
                            "release": "minor"
                        },
                        {
                            "type": "feat",
                            "release": "minor"
                        },
                        {
                            "type": "feature",
                            "release": "minor"
                        },
                        {
                            "scope": "no-release",
                            "release": false
                        }
                    ]
                }
            ],
            [
                "@semantic-release/release-notes-generator"
            ],
            [
                "@semantic-release/github",
                {
                    "successComment": false,
                    "failComment": false
                }
            ],
            [
                "@semantic-release/npm"
            ],
            [
                "@semantic-release/git",
                {
                    "assets": [
                        "package.json"
                    ]
                }
            ]
        ]
    }
}
